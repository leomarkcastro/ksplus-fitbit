{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "baseUrl": "./",
    "paths": {
      "~graphql/*": ["graphql/*"],
      "~modules/*": ["src/modules/*"],
      "~/*": ["src/*"]
    }
  },
  "include": ["src/**/*", "graphql/**/*"]
}
