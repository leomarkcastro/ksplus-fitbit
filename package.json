{
  "name": "keystone-app",
  "version": "1.0.2",
  "private": true,
  "scripts": {
    "dev:unsafe": "keystone dev",
    "dev:safe": "npm run generate-types && npm run migrate:dev && npm run dev:nodbpush",
    "dev:start": "npm run dev:nodbpush",
    "dev:nodbpush": "keystone dev --no-db-push",
    "start:prod:win": "set NODE_ENV=production && keystone start",
    "build:prod:win": "set NODE_ENV=production && keystone build",
    "start:prod:docker": "NODE_ENV='production' PORT='80'  keystone start",
    "build:prod:docker": "NODE_ENV=production keystone build",
    "migrate:dev": "keystone prisma migrate dev",
    "migrate:prod": "keystone prisma migrate deploy",
    "generate-types": "npm run codegen-env && keystone postinstall --fix && keystone postinstall && npm run codegen",
    "prepare": "husky",
    "format": "prettier --write \"./**/*.ts\"",
    "sample-env": "ts-node --transpile-only ./utils/automations/env-sync.ts",
    "codegen-env": "ts-node --transpile-only ./utils/automations/env-codegen.ts",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.0.0",
    "@graphql-codegen/cli": "5.0.2",
    "@graphql-codegen/typed-document-node": "^5.0.6",
    "@graphql-codegen/typescript": "4.0.6",
    "@graphql-codegen/typescript-document-nodes": "4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@graphql-codegen/typescript-resolvers": "4.0.6",
    "@graphql-tools/schema": "^10.0.3",
    "@keystone-6/auth": "^7.0.0",
    "@keystone-6/core": "^5.0.0",
    "@keystone-6/fields-document": "^7.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node-cron": "^3.0.11",
    "@types/swagger-ui-express": "^4.1.6",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "jsonwebtoken": "^9.0.2",
    "moment": "^2.30.1",
    "moment-timezone": "^0.5.45",
    "node-cron": "^3.0.3",
    "prettier": "^3.2.5",
    "sib-api-v3-typescript": "^2.2.2",
    "swagger-ui-express": "^5.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/node-fetch": "^2.6.11",
    "husky": "^9.0.11"
  }
}
